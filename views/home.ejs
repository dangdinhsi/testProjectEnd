                                <a class="product_left_choose-link" href="/detail/<%=top1_product[0].Id%>">
                                    <i class="product_left_choose-icon fas fa-eye"></i>
                                </a>
                            </li>
                            <li class="product_left_choose-list">
                                <a class="product_left_choose-link" href="#">
                                    <i class="product_left_choose-icon fas fa-cart-plus"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="grid__colum-8">
                <div class="grid__row">
                    <% for (var item of home_bestsell){ %>
                        <div class="grid__colum-6">
                            <div class="best_seller_right_product">
                                <div class="best_seller_right_product_overlay"></div>
                                <img class="best_seller_right_product-img" src="<%=item.DefaultImage%>" alt="">
                                <div class="best_seller_right_product-detail">
                                    <a href="/detail/<%=item.Id%>" class="best_seller_right_product-title"><%=item.Name%></a>
                                    <span class="best_seller_right_product-price">$<%=item.Price%></span>
                                    <div class="best_seller_right_product-action">
                                        <div class="best_seller_right_product-action-star">
                                            <a href="#"><i class="fas fa-star"></i></a>
                                            <a href="#"><i class="fas fa-star"></i></a>
                                            <a href="#"><i class="fas fa-star"></i></a>
                                            <a href="#"><i class="fas fa-star"></i></a>
                                            <a href="#"><i class="fas fa-star"></i></a>
                                        </div>
                                        <a class="best_seller_right_product-action-star_cart" href="#"><i class="fas fa-cart-plus"></i></a>
                                    </div>

                                </div>
                            </div>
                        </div>
                    <% } %>

                </div>
            </div>
